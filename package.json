    "test:updateSnapshot": "turbo run test:updateSnapshot",
    "build": "turbo run build"
  },
  "packageManager": "npm@11.4.2",
  "volta": {
    "node": "22.17.0",
    "npm": "11.4.2"
  },
  "workspaces": [
    "packages/*"
  ]
