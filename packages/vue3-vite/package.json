{
  "name": "vue3-vite",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "test:unit": "vitest --environment jsdom",
    "lint": "biome lint ./src",
    "lint:fix": "biome check --write ./src",
    "dev": "vite",
    "preview": "vite preview --port 5050",
    "test": "vitest --run --coverage --silent --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "pinia": "3.0.4",
    "roboto-fontface": "0.10.0",
    "vue": "3.5.27",
    "vue-router": "4.6.4",
    "vuetify": "3.11.8",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@types/jsdom": "27.0.0",
    "@types/node": "24.10.9",
    "@types/webfontloader": "1.6.38",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitejs/plugin-vue-jsx": "5.1.3",
    "@vitest/coverage-v8": "4.0.18",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "jsdom": "27.4.0",
    "sass": "1.97.3",
    "sass-loader": "16.0.6",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-vuetify": "2.1.3",
    "vitest": "4.0.18",
    "vue-cli-plugin-vuetify": "2.5.8",
    "vue-tsc": "3.2.4"
  }
}