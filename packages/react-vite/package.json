{
  "name": "react-vite",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "format": "biome format ./src --write",
    "lint": "biome lint ./src --files-ignore-unknown=true",
    "lint:fix": "biome check --apply ./src --files-ignore-unknown=true",
    "dev": "vite",
    "dev:emulator": "vite --mode emulator",
    "build": "tsc --noEmit && vite build",
    "analyze": "ANALYZE=true npm run build",
    "serve": "vite preview",
    "test": "jest",
    "test:updateSnapshot": "npm run test -- -u",
    "emulators:start": "npx firebase emulators:start --import=./firebase/firestore-data",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.6",
    "@mui/material": "7.3.6",
    "@mui/x-data-grid": "8.21.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-firebase-hooks": "5.1.1",
    "react-router-dom": "7.10.1"
  },
  "devDependencies": {
    "@mdx-js/preact": "3.1.1",
    "@storybook/addon-essentials": "9.0.0-alpha.12",
    "@storybook/addon-interactions": "9.0.0-alpha.10",
    "@storybook/addon-links": "10.1.2",
    "@storybook/builder-vite": "10.1.2",
    "@storybook/react": "10.1.2",
    "@storybook/react-vite": "10.1.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "babel-jest": "30.2.0",
    "babel-loader": "10.0.0",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.1.0",
    "esbuild-register": "3.6.0",
    "firebase": "12.6.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-resolve": "30.2.0",
    "jest-watch-typeahead": "3.0.1",
    "prop-types": "15.8.1",
    "rc": "1.2.8",
    "rollup-plugin-visualizer": "6.0.5",
    "storybook": "10.1.2",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "vite": "7.2.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
